{
  "name": "babel-plugin-transform-slow-func-detecter",
  "version": "2.1.3",
  "description": "分析js运行过程中是否存在慢方法",
  "main": "lib/index.js",
  "scripts": {
    "dev": "rm -rf lib && tsc -w",
    "build": "rm -rf lib && tsc",
    "babel": "babel ./src --watch --out-dir lib --extensions '.ts,.tsx,.js'",
    "exe": "node lib/index.js"
  },
  "repository": "https://github.com/SBDavid/babel-plugin-slow-func-detecter",
  "homepage": "https://github.com/SBDavid/babel-plugin-slow-func-detecter#readme",
  "author": "jiawei.tang <billl89@126.com> (https://github.com/SBDavid)",
  "license": "ISC",
  "dependencies": {
    "@babel/helper-plugin-utils": "^7.14.5"
  },
  "devDependencies": {
    "@types/babel__helper-plugin-utils": "^7.10.0",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0"
  }
}
